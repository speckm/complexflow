{
  "name": "complexflow",
  "version": "0.2.0",
  "description": "Run complex async function scenarios with ease",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "jest",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speckm/complexflow.git"
  },
  "keywords": [
    "async",
    "typescript",
    "scheduler"
  ],
  "author": "M-S",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/speckm/complexflow/issues"
  },
  "homepage": "https://github.com/speckm/complexflow#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm test"
    }
  },
  "dependencies": {
    "lodash.intersection": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.pickby": "^4.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.2.3",
    "@types/lodash.intersection": "^4.4.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.pickby": "^4.6.6",
    "@types/node": "^13.13.16",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "25.2.7",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "semantic-release": "^17.1.1",
    "ts-jest": "25.2.1",
    "typescript": "^3.9.7"
  },
  "files": [
    "lib/**/*"
  ]
}
